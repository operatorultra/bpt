{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/remix@latest",
			"config": {
				"buildCommand": "npm run build",
				"outputDirectory": "build/client",
				"serverless": true
			}
		}
	],
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "build/server/index.js"
		}
	]
}
