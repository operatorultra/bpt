{
	"version": 2,
	"buildCommand": "npm run vercel-build",
	"outputDirectory": "build/client",
	"routes": [
		{ "handle": "filesystem" },
		{ "src": "/build/(.*)", "dest": "/build/$1" },
		{ "src": "/assets/(.*)", "dest": "/assets/$1" },
		{ "src": "/(.*)", "dest": "/server.js" }
	],
	"functions": {
		"server.js": {
			"runtime": "nodejs18.x"
		}
	}
}
